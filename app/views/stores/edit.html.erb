<h1> お店編集 </h1>
<%= form_for (@store) do |f| %>
<h4> 店名</h4>
<%= f.text_field :storename %>

<h4> 写真</h4>
<%=  f.attachment_field :image %>
<h4> 場所</h4>
<%= f.text_field :city %>
<h4> 動物カテゴリー</h4>
<%= f.text_field :category %>
<h4> 料金システム</h4>
<%=	f.text_area :system %>
<h4> 住所</h4>
<%= f.text_field :place %>
<h4> Tag</h4>

<%= f.submit '保存' %>
<% end %>